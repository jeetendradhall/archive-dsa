    "def toi (n, a, b, c):\n",
    "    if n == 1:\n",
    "        print ('Move 1 from', a, 'to', b)\n",
    "        return\n",
    "    \n",
    "    toi (n-1, a, c, b)\n",
    "    print ('Move', n, 'from', a, 'to', b)\n",
    "    toi (n-1, c, b, a)\n",
    "\n",
    "toi (3, 'A', 'B', 'C')"
